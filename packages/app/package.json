    "pino": "^9.5.0",
    "probot": "^13.3.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^7.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "@types/pino": "^7.0.5",
    "@vitest/ui": "^2.1.9",
